@model ResumeManagement.Models.Qualification

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">
                <h4>Qualification Details</h4>
            </div>
            <div class="card-body">
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.QualificationName)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.QualificationName)
                    </dd>

                </dl>
            </div>
            <div class="card-footer">
                @Html.ActionLink("Edit", "Edit", new { id = Model.QualificationId }, new {@class="btn btn-info"}) 
                @Html.ActionLink("Back to List", "Index", null, new {@class="btn btn-primary"})
            </div>

        </div>
    </div>
</div>

